{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es5",
      "allowSyntheticDefaultImports": true,
      "lib": [
          "es2017", "es2016", "es2015", "dom"
      ],
      "types": [
          "node", "express", "jquery"
      ]
    },
    "include": [
        "src/server/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
  }